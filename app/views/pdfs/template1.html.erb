<h1>PDFエクスポートの例 1</h1>
<table>
  <thead>
    <tr>
      <% data.first.keys.each do |key| %>
        <th><%= key.capitalize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% data.each do |item| %>
      <tr>
        <% item.values.each do |value| %>
          <td><%= value %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
