<h1>PDFエクスポートの例 2</h1>
<table>
  <thead>
    <tr>
      <% data[:users].first.keys.each do |key| %>
        <th><%= key.capitalize %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
    <% data[:users].each do |item| %>
      <tr>
        <% item.values.each do |value| %>
          <td><%= value %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
