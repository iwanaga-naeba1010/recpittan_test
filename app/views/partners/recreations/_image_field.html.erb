<div class="nested-fields row">
  <% if f.object.image.to_s.present? %>
    <%= image_tag f.object.image.to_s %>
  <% else %>
    <%= f.input :image, as: :file %>
  <% end %>
  <div class="cocoon">
    <% remove_button = capture do %>
      sakujo
      <%= link_to_remove_association remove_button, f %>
    <% end %>
  </div>
</div>

