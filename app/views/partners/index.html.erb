<% @orders.each do |order| %>
  <%= link_to chat_partners_order_path(order.id) do %>
    <div class="order-wrapper my-2 border-bottom p-3">
      <div class="company_name">
        <h3><%= order.recreation.user.partner.name %></h3>
      </div>
      <div class="partner_name">
        <%= order.recreation.title %>
      </div>
      <div class="status">
        <span class="status__cunsult p-1">
          相談したい
        </span>
      </div>
      <div class="date">
        11/03
      </div>
    </div>
  <% end %>
<% end %>
